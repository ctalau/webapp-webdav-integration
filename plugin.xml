<?xml version="1.0" encoding="UTF-8"?>
<!-- The plugin.dtd file is located in the OXYGEN_INSATALL_DIR/plugins directory -->
<!DOCTYPE plugin SYSTEM "../plugin.dtd">

<plugin
 id="com.oxygenxml.examples.webdav.CustomProtocol"
 name="WebDAVCustomProtocol"
 description="An example of a custom protocol implementation that connects the user to a WevDAV server."
 version="${project.version}"
 vendor="SyncRO"
 class="com.oxygenxml.examples.webdav.WebdavPlugin"
 classLoaderType="preferOxygenResources">
 <runtime>
  <librariesFolder name="lib" scope="global"/>
 </runtime>
 
 <extension type="URLHandler" 
  class="com.oxygenxml.examples.webdav.WebdavURLHandlerExtension"/>
 <extension type="WebappServlet" 
  class="com.oxygenxml.examples.webdav.LoginServlet"/>
</plugin>
